        <TabControl>
            <TabItem Header="Application 1">
        <local:ParentProcessDemoUserControl x:Name="DemoControl1"></local:ParentProcessDemoUserControl>
            </TabItem>
            <TabItem Header="Application 2">
            <local:ParentProcessDemoUserControl x:Name="DemoControl2"></local:ParentProcessDemoUserControl>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
